{
    "name": "denary-api",
    "version": "1.0.0",
    "description": "An API built with NodeJS, Express, Knex and TypeScript",
    "main": "dist/server.js",
    "scripts": {
        "test": "tsc && jest ./dist/lib/tests",
        "docker-test": "cross-env NODE_ENV=test docker-compose up --abort-on-container-exit --exit-code-from api",
        "build": "tsc",
        "go-dev": "npm run build:live",
        "go-dev-win": "npm run build:live:win",
        "start": "sh ./scripts/start",
        "prod": "npm run build && npm run start",
        "migrate-win": "npm run-script migrate",
        "migrate-linux": "npm run-script migrate",
        "migrate": "node_modules/.bin/rimraf dist && npm run build && cross-env node_modules/.bin/ts-node node_modules/typeorm/cli.js migration:run",
        "build:live": "./node_modules/.bin/nodemon --watch 'lib/**/*.ts' --exec ts-node lib/api/server.ts",
        "build:live:win": "./node_modules/.bin/nodemon --exec ts-node ./lib/api/server.ts"
    },
    "keywords": [
        "Echo",
        "HR"
    ],
    "author": "Apps-garden Sp.Z.o.o",
    "dependencies": {
        "@types/async": "^2.4.1",
        "@types/bcrypt-nodejs": "0.0.30",
        "@types/dotenv": "^6.1.1",
        "@types/http-status-codes": "^1.2.0",
        "@types/joi": "^14.3.2",
        "@types/jsonwebtoken": "^8.3.2",
        "@types/morgan": "^1.7.35",
        "@types/node": "^10.14.4",
        "@types/pg": "^7.4.14",
        "@types/swagger-ui-express": "^3.0.0",
        "@types/url-parse": "^1.4.3",
        "async": "^2.6.2",
        "base64topdf": "^1.1.8",
        "bcrypt": "^3.0.5",
        "bcrypt-nodejs": "0.0.3",
        "cors": "^2.8.5",
        "del": "^3.0.0",
        "dotenv": "^6.2.0",
        "express": "^4.16.4",
        "http-status-codes": "^1.3.2",
        "joi": "^14.3.1",
        "joi-date-extensions": "^1.2.0",
        "json2xls": "^0.1.2",
        "jsonwebtoken": "^8.5.1",
        "log4js": "^6.3.0",
        "lokijs": "^1.5.6",
        "moment": "^2.24.0",
        "morgan": "^1.9.1",
        "multer": "^1.4.1",
        "node-env-file": "^0.1.8",
        "nodemailer": "^5.1.1",
        "nodemon": "^1.18.10",
        "pdfmake": "^0.1.54",
        "pg": "^7.9.0",
        "reflect-metadata": "^0.1.13",
        "serialport": "^9.0.1",
        "swagger-ui-express": "^4.0.2",
        "typeorm": "^0.2.16",
        "typescript-logging": "^0.6.3",
        "uuidv4": "^4.0.0",
        "ws": "^7.3.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.4",
        "@types/del": "^3.0.1",
        "@types/express": "^4.16.1",
        "@types/jest": "^23.3.14",
        "@types/lokijs": "^1.5.2",
        "@types/multer": "^1.3.7",
        "@types/nodemailer": "^4.6.7",
        "@types/supertest": "^2.0.7",
        "@types/uuid": "^3.4.4",
        "cross-env": "^5.2.0",
        "jest": "^24.7.1",
        "supertest": "^3.4.2",
        "ts-jest": "^23.10.5",
        "ts-node": "^7.0.1",
        "tsconfig-paths": "^3.8.0",
        "typescript": "^3.4.2"
    }
}
